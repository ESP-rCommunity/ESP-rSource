<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <apply_style_sheet>false</apply_style_sheet>
  <dump_all_data>false</dump_all_data>
  <hierarchy>flat</hierarchy>
  <link_style_sheet>false</link_style_sheet>
  <output_dictionary>true</output_dictionary>
  <report_startup_period_data>false</report_startup_period_data>
  <save_to_disk>false</save_to_disk>
  <time_step_averaging>false</time_step_averaging>
  <!--Allow regexp-like wildcard matching for list of output variables-->
  <enable_xml_wildcards>true</enable_xml_wildcards>
  <!--List of data to appear in out.xml-->
  <log_variable>building/zone_1/air_point/temperature</log_variable>
  <log_variable>climate/dry_bulb_temperature</log_variable>
  <log_variable>climate/sky_temperature</log_variable>
  <!--List of data to appear in out.csv-->
  <step_variable>building/month</step_variable>
  <step_variable>building/day/present</step_variable>
  <step_variable>building/day_number/present</step_variable>
  <step_variable>building/hour/present</step_variable>
  <step_variable>building/time/present</step_variable>
  <step_variable>building/time_step</step_variable>
  <step_variable>building/zone_1/air_point/temperature</step_variable>
  <step_variable>building/zone_1/surface_1/temperature</step_variable>
  <step_variable>building/zone_3/air_point/temperature</step_variable>
  <!-- flow network -->
  <step_variable>mfn/air/TheSpace/flowrate</step_variable>
  <step_variable>mfn/air/hungCeiling/flowrate</step_variable>
  <step_variable>mfn/air/mixBottom/flowrate</step_variable>
  <step_variable>mfn/air/TheChannel/flowrate</step_variable>
  <step_variable>mfn/air/TheChannel/volflowrate</step_variable>
  <step_variable>mfn/air/TheChannel/temp</step_variable>
  <step_variable>mfn/air/mixTop/flowrate</step_variable>
  <step_variable>mfn/air/ext_bot/flowrate</step_variable>
  <step_variable>mfn/air/ext_bot->mixBottom (via ext_openings)/veloc</step_variable>
  <step_variable>mfn/air/ext_top/flowrate</step_variable>
  <step_variable>mfn/air/ext_top->mixTop (via ext_openings)/veloc</step_variable>
  <step_variable>mfn/air/mixBottom->TheChannel (via int_large)/Dp</step_variable>
  <step_variable>mfn/air/mixBottom->TheChannel (via int_large)/flow</step_variable>
  <step_variable>mfn/air/mixBottom->TheChannel (via int_large)/veloc</step_variable>
  <step_variable>mfn/air/TheChannel->mixTop (via int_large)/Dp</step_variable>
  <step_variable>mfn/air/TheChannel->mixTop (via int_large)/flow</step_variable>
  <step_variable>mfn/air/TheChannel->mixTop (via int_large)/veloc</step_variable>
  <!-- climate -->
  <step_variable>climate/solar/direct_normal_radiation</step_variable>
  <step_variable>climate/solar/diffuse_horizontal_radiation</step_variable>
  <step_variable>climate/dry_bulb_temperature</step_variable>
  <step_variable>climate/sky_temperature</step_variable>
  <step_variable>climate/sky_temperature_depression</step_variable>
  <step_variable>climate/cloud_cover</step_variable>
  <step_variable>climate/ambient_air_tsat</step_variable>
  <step_variable>climate/relative_humidity</step_variable>
</configuration>
