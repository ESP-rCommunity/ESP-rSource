*idealized-hvac
*version 2
#------------------------
# Site altitude 
#------------------------
*site-altitude 0.0
#------------------------
# First system (IMS)
#------------------------
*system-start
*name           IMS
*type           IMS            # System type: IMS
*priority       primary        # Priority: primary
*fuel           electricity
*function       heating
*zone-links-start
*zone-link 1  0.5         # Served zones and fraction
*zone-link 2  0.5         #
*zone-links-end 
*sys-data-start
*sys-data-end
*system-end
#------------------------
# Second system (furnace)
#------------------------
*system-start
*name           furnace
*type           furnace       # System type: IMS
*priority       backup        # Priority: primary
*fuel           gas
*function       heating
*zone-links-start
*zone-link 1 0.5               # Served zones and fraction
*zone-link 2 0.5               #
*zone-links-end 
*sys-data-start
  furnace-type      14
  capacity          32000
  efficiency        0.68
  circ-fan-mode     1
  circ-fan-power    500.0
  draft-fan-power   50.0
  pilot-fan-power   0.
*sys-data-end
*system-end
#------------------------
# Third system (ASHP-heating)
#------------------------
*system-start
*name           ASHP-heating
*type           ASHP          # System type: IMS
*priority       primary       # Priority: primary
*fuel           electricity
*function       heating
*zone-links-start
*zone-link 1 0.5               # Served zones and fraction
*zone-link 2 0.5               #
*zone-links-end 
*sys-data-start
  performance               1
  capacity                  32000
  COP                       3.2
  circ-fan-flow             1
  circ-fan-rated-flow      -1.0
  circ-fan-position         1
  circ-fan-power           -1.0
  outdoor-fan-power        -1.0
  fan-power-in-auto         0.0
  circ-fan-rated-position   0.0
  circ-ran-rated-power      0.
  temp-control-mode         1
  cut-off-temperature       26
*sys-data-end
*backup                     furnace
*system-end
#------------------------
# Forth system (ASHP-cooling-conventional)
#------------------------
*system-start
  *name           ASHP-cooling
  *type           ASHP          # System type: IMS
  *priority       primary       # Priority: primary
  *fuel           electricity
  *function       cooling 
  *zone-links-start
  *zone-link 1 0.5               # Served zones and fraction
  *zone-link 2 0.5               #
  *zone-links-end 
  *sys-data-start
    performance               1
    capacity                  32000
    COP                       3.2
    circ-fan-flow             1
    circ-fan-rated-flow      -1.0
    circ-fan-position         1
    circ-fan-power           -1.0
    outdoor-fan-power        -1.0
    fan-power-in-auto         0.0
    circ-fan-rated-position   0.0
    circ-ran-rated-power      0.
    SHR                       1
    cooling-control-mode      1
  *sys-data-end
  *OA-schedule-start
    *day-type-start
      *end-hour  2000
      *period    8    0
      *period    20   0
      *period    24   0
    *day-type-end
    *day-type-start
      *end-hour  8760
      *period    8    0
      *period    20   0
      *period    24   0
    *day-type-end
  *OA-schedule-end
*system-end
#------------------------
# Fifth system (ASHP-cooling-economizer)
#------------------------
*system-start
*name           ASHP-cooling
*type           ASHP          # System type: IMS
*priority       primary       # Priority: primary
*fuel           electricity
*function       cooling 
*zone-links-start
*zone-link 1 0.5               # Served zones and fraction
*zone-link 2 0.5               #
*zone-links-end 
*sys-data-start
  performance               1
  capacity                  32000
  COP                       3.2
  circ-fan-flow             1
  circ-fan-rated-flow      -1.0
  circ-fan-position         1
  circ-fan-power           -1.0
  outdoor-fan-power        -1.0
  fan-power-in-auto         0.0
  circ-fan-rated-position   0.0
  circ-ran-rated-power      0.
  SHR                       1
  cooling-control-mode      2
  econ-control-type         4
  econ-min-OA-flow          25
  econ-space-set-point      20
  econ-OA-temp-limit        25
  econ-OA-enth-limit        2505
  sys-num-for-heating-mode  2
  ctl-file-function-num     1
*sys-data-end
*system-end
#------------------------
# sixth system (Baseboard-heating)
#------------------------
*system-start
*name           baseboard
*type           baseboard      # System type: IMS
*priority       primary        # Priority: primary
*fuel           electricity
*function       heating
*zone-links-start
*zone-link 1  0.5         # Served zones and fraction
*zone-link 2  0.5         #
*zone-links-end 
*sys-data-start
  capacity          32000
  efficiency        0.68
  circ-fan-mode     1
  circ-fan-power    500.0
*sys-data-end
*system-end